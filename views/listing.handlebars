{{#each reviews}}
<div class="row">
  <div class="col s12 m6">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">{{ category }}</span>
      </div>
      <div class="card-action">
        <p> {{ company }} </p>
        <p> {{ address }} </p>
        <p> {{ telephone }} </p>
        <p> {{ description }} </p>
      </div>
      <div>
        <a href="/place/{{category}}/{{id}}">Reviews</a>
      </div>
    </div>
  </div>
</div>
{{/each}}



